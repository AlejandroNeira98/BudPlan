<h1>Groups#new</h1>
<p>Find me in app/views/groups/new.html.erb</p>

<%= form_with(model: @group, url: groups_create_path ) do |form| %>

  <div>
    <%= form.label :name %>
    <%= form.text_field :name%>
  </div>

  <div>
    <%= form.label :icon %>
    <%= form.file_field :icon%>
  </div>
  
  <%= form.submit "SUBMIT" %>
<% end %>